		<script>
	function sendRequest(error, source, lineno, colno) {
		var params = {
			product: 'web',
			event: 'error_js',
			page_type: 'app_post_download',
			dl_type: '',
			ph: 'Windows',
			title: 'conceptdraw',
			id_file: '98940',
			user_agent: navigator.userAgent,
			page_url: window.location.href,
			error_line: lineno + ':' + colno,
			error_source: source,
			error_stack: error.stack,
			error_message: error.message,
			error_name: error.name
		};

		var queryString = Object.keys(params)
			.map(function(key) {
				return key + '=' + encodeURIComponent(params[key]);
			})
			.join('&');
		var url = 'https://softonic-analytics.net/blank.gif?' + queryString;
		var xmlhttp = new XMLHttpRequest();
		xmlhttp.open('GET', url, true);
		xmlhttp.send();
	}

	window.onerror = function(messageOrEvent, source, lineno, colno, error) {
		var isSoftonicCdn = /m\.sftcdn\.net/.test(source);
		if (isSoftonicCdn) {
			sendRequest(error || {
				message: messageOrEvent
			}, source, lineno, colno);
		}
	}
</script>

		
